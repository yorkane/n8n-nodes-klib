{
  "name": "n8n-nodes-klib",
  "version": "0.1.14",
  "description": "A collection of useful n8n nodes for file system operations, aria2 integration, image processing and torrent parsing",
  "keywords": [
    "n8n-community-node-package",
    "file-system",
    "fs",
    "aria2",
    "image-processing",
    "torrent",
    "jieba",
    "chinese",
    "segmentation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yorkane/n8n-nodes-klib",
  "author": {
    "name": "Yorkane",
    "email": "yorkane@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yorkane/n8n-nodes-klib.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
    ],
    "nodes": [
      "dist/nodes/ImageProcessor/ImageProcessor.node.js",
      "dist/nodes/Aria2/Aria2.node.js",
      "dist/nodes/Aria2/Aria2Proc.node.js",
      "dist/nodes/TorrentParse/TorrentParse.node.js",
      "dist/nodes/FsUtils/FsUtils.node.js",
      "dist/nodes/Jieba/JiebaSegment.node.js"
    ]
  },
  "dependencies": {
    "@node-rs/jieba": "^2.0.1",
    "@types/mime-types": "^2.1.4",
    "axios": "^1.6.7",
    "pnpm": "^10.9.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-svgmin": "^4.1.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
